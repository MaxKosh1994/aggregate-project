{
  "name": "wishlist-api",
  "version": "1.0.1",
  "description": "Система управления списком желаний с поддержкой REST API, аутентификации, валидации данных через OpenAPI и работы с базой данных.",
  "main": "app.js",
  "scripts": {
    "start": "node ./app.js",
    "test": "npx jest --detectOpenHandles",
    "dev": "nodemon ./app.js --ext js,json,css,jsx",
    "lint": "eslint .",
    "db:init": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "reset": "npx sequelize-cli db:drop",
    "swagger:docs": "node ./swagger.js",
    "clean-install": "rm -rf node_modules && rm -f package-lock.json && npm install"
  },
  "keywords": [
    "wishlist",
    "express",
    "sequelize",
    "swagger",
    "REST API"
  ],
  "author": "Max Kosh",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@faker-js/faker": "^9.3.0",
    "eslint": "^9.17.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "glob": "^10.3.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemon": "^3.1.7",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "prom-client": "^15.1.3",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ws": "^8.18.0",
    "yamljs": "^0.3.0"
  }
}
